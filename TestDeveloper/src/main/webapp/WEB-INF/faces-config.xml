<?xml version="1.0" encoding="UTF-8"?>
<faces-config
    xmlns="http://xmlns.jcp.org/xml/ns/javaee"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-facesconfig_2_3.xsd"
    version="2.3">
    
   <navigation-rule>
        <from-view-id>/index.html</from-view-id>
        <navigation-case>
            <from-outcome>teste</from-outcome>
            <to-view-id>/teste.xhtml</to-view-id>
        </navigation-case>
    </navigation-rule>
    <navigation-rule>
        <from-view-id>/clientes.xhtml</from-view-id>
        <navigation-case>
            <from-outcome>clientes</from-outcome>
            <to-view-id>/teste.xhtml</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-outcome>voltar</from-outcome>
            <to-view-id>/index.html</to-view-id>
        </navigation-case>
    </navigation-rule>
    <navigation-rule>
        <from-view-id>/teste.xhtml</from-view-id>
        <navigation-case>
            <from-outcome>voltar</from-outcome>
            <to-view-id>/index.html</to-view-id>
        </navigation-case>
    </navigation-rule>

</faces-config>
